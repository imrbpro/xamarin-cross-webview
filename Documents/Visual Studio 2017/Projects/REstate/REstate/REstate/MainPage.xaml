<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:REstate"
             x:Class="REstate.MainPage">

    <StackLayout >
        <StackLayout Orientation="Horizontal">
            <Button Text="Page1" HorizontalOptions="StartAndExpand" Clicked="p1Clicked" />
            <Button Text="Page2" HorizontalOptions="CenterAndExpand" Clicked="p2Clicked" />
            <Button Text="Page3" HorizontalOptions="CenterAndExpand" Clicked="p3Clicked" />
            <Button Text="Page4" HorizontalOptions="EndAndExpand" Clicked="p4Clicked" />
        </StackLayout>
        <!-- Place new controls here -->
        <WebView x:Name="UrlBrowse" WidthRequest="1000" HeightRequest="1000" ></WebView>
    </StackLayout>

</ContentPage>
